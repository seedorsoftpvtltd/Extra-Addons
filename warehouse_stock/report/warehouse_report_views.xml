<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="warehouse_report_view_search" model="ir.ui.view">
        <field name="name">warehouse.report.search.stock</field>
        <field name="model">warehouse.report</field>
        <field name="inherit_id" ref="warehouse.view_warehouse_order_search"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='user_id']" position="after">
                <field name="picking_type_id"/>
            </xpath>
        </field>
    </record>

</odoo>
