<?xml version="1.0" encoding="utf-8" ?>

<odoo>

    <record id="warehouse_field_form_inherited" model="ir.ui.view">
        <field name="name">warehouse.order.inherited</field>
        <field name="model">warehouse.order</field>
        <field name="inherit_id" ref="warehouse.warehouse_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='currency_id']" position="after">
                <field name="x_billno"/>
            </xpath>
            <xpath expr="//field[@name='origin']" position="after">
                <field name="x_transport"/>

            </xpath>
        </field>
    </record>


</odoo>