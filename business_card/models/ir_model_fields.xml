    <record id="partner_business_card_count_field" model="ir.model.fields">
        <field name="complete_name" eval="False"/>
        <field name="compute"><![CDATA[
business_card_obj = self.env['x_business.card']
for card in self:
    card['x_business_card_count'] = business_card_obj.search_count(['|',('x_parent_id', '=', card.id),('x_partner_id','=',card.id)])
]]></field>
        <field name="copied" eval="True"/>
        <field name="depends" eval="False"/>
        <field name="domain">[]</field>
        <field name="field_description">Number of Business Card</field>
        <field name="groups" eval="[(6, 0, [])]"/>
        <field name="help" eval="False"/>
        <field name="index" eval="True"/>
        <field name="model">res.partner</field>
