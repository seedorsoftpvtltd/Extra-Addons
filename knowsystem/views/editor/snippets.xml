<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Snippets & Themes Menu -->
    <template id="knowsystem_snippets"
              inherit_id="web_editor.snippets"
              primary="True"
    >
        <xpath expr="//div[@id='o_scroll']" position="replace">
            <div id="o_scroll">
                <div id="snippet_structure" class="o_panel">
                    <div class="o_panel_header">
                        <i class="fa fa-paragraph"/> Texts
                    </div>
                    <div class="o_panel_body">
                        <t t-snippet="knowsystem.knowsystem_block_title_text"
                           t-thumbnail="/knowsystem/static/src/img/blocks/title_text.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_block_text"
                           t-thumbnail="/knowsystem/static/src/img/blocks/paragraph.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_block_tilte_subtitle"
                           t-thumbnail="/knowsystem/static/src/img/blocks/title_subtitle.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_comparison"
                           t-thumbnail="/knowsystem/static/src/img/blocks/comparison.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_2_columns_text"
                           t-thumbnail="/knowsystem/static/src/img/blocks/2_columns_text.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_3_columns_text"
                           t-thumbnail="/knowsystem/static/src/img/blocks/3_columns_text.png"
                        />
                    </div>
                    <div class="o_panel_header">
                        <i class="fa fa-plus"/> Special
                    </div>
                    <div class="o_panel_body">
                        <t t-snippet="knowsystem.knowsystem_card"
                           t-thumbnail="/knowsystem/static/src/img/blocks/card.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_alert"
                           t-thumbnail="/knowsystem/static/src/img/blocks/alert.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_code"
                           t-thumbnail="/knowsystem/static/src/img/blocks/code.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_recommendations"
                           t-thumbnail="/knowsystem/static/src/img/blocks/recommend.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_separator"
                           t-thumbnail="/knowsystem/static/src/img/blocks/separator.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_tag_line"
                           t-thumbnail="/knowsystem/static/src/img/blocks/tag_line.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_steps" 
                           t-thumbnail="/knowsystem/static/src/img/blocks/steps.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_footer" 
                           t-thumbnail="/knowsystem/static/src/img/blocks/footer.png"
                        />
                    </div>
                    <div class="o_panel_header">
                        <i class="fa fa-photo"/> Images
                    </div>
                    <div class="o_panel_body">
                        <t t-snippet="knowsystem.knowsystem_2_columns"
                           t-thumbnail="/knowsystem/static/src/img/blocks/2_columns.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_3_columns"
                           t-thumbnail="/knowsystem/static/src/img/blocks/3_columns.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_image_text"
                           t-thumbnail="/knowsystem/static/src/img/blocks/image_text.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_text_image"
                           t-thumbnail="/knowsystem/static/src/img/blocks/text_image.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_image"
                           t-thumbnail="/knowsystem/static/src/img/blocks/image.png"
                        />
                        <t t-snippet="knowsystem.knowsystem_image_banner" 
                           t-thumbnail="/knowsystem/static/src/img/blocks/banner.png"
                        />
                    </div>
                </div>
            </div>
        </xpath>
        <xpath expr="//div[@id='snippet_options']" position="inside">
            <t t-call="knowsystem.snippet_options"/>
        </xpath>
    </template>

    <!-- Texts -->
    <template id="knowsystem_block_title_text" name="Title + Text">
        <div class="knowsystem_block_title_text">
            <div class="knowsystem_snippet_general">
                <table align="center"
                       cellspacing="0"
                       cellpadding="0"
                       border="0"
                       class="knowsystem_table_styles"
                >
                    <tbody>
                        <tr>
                            <td class="knowsystem_h_padding knowsystem_v_padding">
                                <h2 class="knowsystem_no_margin"><span>Title</span></h2>
                                <p>point 1</p>
                                <p>point 2</p>
                                <p>point 3</p>
                            </td>
                        </tr>
                        <tr/>
                    </tbody>
                </table>
            </div>
        </div>
    </template>

