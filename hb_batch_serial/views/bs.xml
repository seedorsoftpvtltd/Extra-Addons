<?xml version="1.0" encoding="utf-8"?>
<odoo>

	 <record model='ir.ui.view' id='stock_pickingview_inht'>
        <field name="name">stock.picking.view</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">

            <xpath expr="//field[@name='move_ids_without_package']/tree/field[@name='product_id']" position="before">
				<field name="expiry_date"/>
				<field name="production_date"/>
			</xpath>


        </field>
       </record>

<record id="hb_stock_move_line_custom_form_view" model="ir.ui.view">
			<field name="name">hb.stock.move.line.custom.form.view</field>
			<field name="model">stock.move.line</field>
			<field name="inherit_id" ref="stock.view_move_line_form"/>
			<field name="arch" type="xml">
				<xpath expr="//sheet//field[@name='lot_id']" position="before">

                       <field name="serialno" />
                       <field name="batchno" />

				</xpath>
			</field>
		</record>
		 <record id="view_move_line_form_hb_inh" model="ir.ui.view">
		<field name="model">stock.move.line</field>
		<field name="inherit_id" ref="stock.view_stock_move_line_detailed_operation_tree"/>
		<field name="arch" type="xml">
			<xpath expr="//field[@name='lot_id']" position="before">
						<field name="move_id" invisible="1"/>
                       <field name="serialno" />
                       <field name="batchno" />

				</xpath>
		</field>
	</record>
