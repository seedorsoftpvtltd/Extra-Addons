<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="button_invoice_number" model="ir.ui.view">
        <field name="name">button.invoice.sequence.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='action_post']" position="after">
                <button name="action_post_seq" string="Post Sequence" class="oe_highlight" type="object"/>
            </xpath>
        </field>
    </record>

</odoo>

