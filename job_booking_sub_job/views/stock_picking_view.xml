<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_picking_form_inherit_etd" model="ir.ui.view">
        <field name="name">view.picking.form.inherit.etd</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='scheduled_date']"
                   position="after">
                <field name="etd"/>
            </xpath>
        </field>
    </record>

</odoo>