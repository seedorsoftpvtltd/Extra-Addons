<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.rule" id="sub_job_multi_company_rule">
        <field name="name">sub job multi company</field>
        <field name="model_id" ref="model_sub_job"/>
        <field name="global" eval="True"/>
        <field name="domain_force">[('company_id', 'in', company_ids)]</field>
    </record>
    <record model="ir.rule" id="freight_multi_company_rule">
        <field name="name">Freight multi company</field>
        <field name="model_id" ref="scs_freight.model_freight_operation"/>
        <field name="global" eval="True"/>
        <field name="domain_force">
            [('company_id', 'in', company_ids)]
        </field>
    </record>
    <record id="group_subjob" model="res.groups">
        <field name="name">Subjob Approval</field>
    </record>
</odoo>