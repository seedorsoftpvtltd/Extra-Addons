<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_purchase_multi_products_wizard" model="ir.ui.view">
            <field name="name">Select Multiple Products Wizard</field>
            <field name="model">purchase.multi.products</field>
            <field name="arch" type="xml">
                <form string="Multi Products Selection" version="7">
                    <group>
                        <field name="do_replace"/>
                    </group>
                    <field name="product_ids"/>
                    <footer>
                        <button name="purchase_multi_products" string="Apply" type="object"
                                class="oe_highlight"/>
                        <button string="_Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
