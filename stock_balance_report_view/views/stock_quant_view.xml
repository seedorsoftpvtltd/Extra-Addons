<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="stock_quant_tree_view" model="ir.ui.view">
        <field name="name">stock quant balance view</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="replace">
                <tree string="Inventory Valuation" js_class="inventory_report_list" create="0" delete="0"
                      decoration-danger="quantity &lt; 0" default_order="sche_date desc">
                    <field name="x_loc_code"/>
                    <field name="x_war_code"/>
                    <field name="x_war_name"/>
                    <field name="x_asn"/>
                    <field name="x_ccode"/>
                    <field name="x_cname"/>
                    <field name="x_sku_code"/>
                    <field name="x_sku_name"/>
                    <field name="x_hs"/>
                    <field name="sche_date"/>
                    <field name="x_grn"/>
                    <field name="x_boe"/>
                    <field name="item_boe"/>
                    <field name="package_id" string="Pallet"/>
                    <field name="location_id"/>
                    <field name="x_ltype" widget="many2many_tags"/>
                    <field name="lot_id" string="Tracking Number"/>
                    <field name="x_barcode"/>
                    <field name="x_uom"/>
                    <field name="x_currency"/>
                    <field name="quantity"/>
<!--                    <field name="net_weight" digits="[5,3]"/>-->
<!--                    <field name="x_weight" digits="[5,3]"/>-->
                    <field name="total_nw" digits="[5,3]"/>
                    <field name="total_gw" digits="[5,3]"/>
                    <field name="x_volume"/>
                    <!--                <field name="x_ex"/>-->
                    <field name="value_goods"/>
                    <field name="x_batchno"/>
                    <field name="x_prod"/>
                    <field name="x_exp"/>
                    <field name="x_coo"/>
                    <field name="putaway_zone"/>
                    <field name="x_sarea" widget="many2many_tags"/>
                    <field name="x_stype"/>
                    <field name="x_container_no"/>
                    <field name="x_tno"/>
                </tree>
            </xpath>
        </field>

    </record>
</odoo>