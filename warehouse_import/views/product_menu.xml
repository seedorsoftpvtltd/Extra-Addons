<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="import_warehouse_order_line_form_inherit" model="ir.ui.view">
            <field name="name">warehouse.order.form.inherit</field>
            <field name="model">warehouse.order</field>
            <field name="inherit_id" ref="warehouse.warehouse_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//header/button[@name='button_done']" position="after">
                    <button name="%(action_import_warehouse_order_line_wizard)d" class="oe_highlight"
                            string="Import warehouse Order Line" type="action"
                            attrs="{'invisible': [('state', '!=', 'draft'),('state', '!=', 'sent'),('state', '!=', 'warehouse')]}"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
