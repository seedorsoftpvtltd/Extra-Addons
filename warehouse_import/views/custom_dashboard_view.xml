<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>

    <record id="custom_dashboard_kanban_viewininh" model="ir.ui.view">
            <field name="name">res.partner.kanban</field>
            <field name="model">custom.dashboard</field>
            <field name="inherit_id" ref="import_bridge_axis.custom_dashboard_kanban_view"/>
            <field name="arch" type="xml">
                <xpath expr='//kanban/templates/t/div/div/strong' position="inside">


                                <div t-if="record.name.value == 'warehouse'">
                                        <div class="o_kanban_image">
                                        <i class="fa fa-clock" style="font-size:48px;color:#208689"></i>
                                    </div>
                                    <div class="oe_kanban_details">
                                    <h2><field name="name"/></h2>
                                    <h2><field name="count"/></h2>
                                         <div class="row col-12">
                                            <button class="col-5 btn btn-primary" type="object" name="action_import_warehouse_order" string="Quotations">Import warehouse Order</button>
                                            <button class="col-5 btn btn-primary" style="margin-left:5px;" name="%(action_export_warehouse_order)d" type="action" string="Quotations"><i class="fa fa-file-excel-o"></i>Sample Files</button>
                                         </div>
                                   </div>
                               </div>
                </xpath>
            </field>
        </record>



</data>
</odoo>
