<odoo>
    <record id="view_import_warehouse_order_wizard" model="ir.ui.view">
        <field name="name">import.warehouse.order.form</field>
        <field name="model">import.warehouse.order</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="file_option" widget="radio"/><div style="height:20px;"></div>

                        <field name="quotation_stage_option" widget="radio"/><div style="height:20px;"></div>
                        <field name="import_file"/>


                    </group>
                    <group>
                        <field name="sequence_option" widget="radio"/><div style="height:10px;"></div>
                        <field name="product_by" widget="radio"/><div style="height:10px;"></div>
                        <field name="product_detail_option" widget="radio"/><div style="height:10px;"></div>


                    </group>
                </group>
                <footer>
                    <button name="import_warehouse_order" string="Import" type="object" default_focus="1" class="oe_highlight"/>
                    <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
    <record model="ir.actions.act_window" id="action_import_warehouse_order">
        <field name="name">Import warehouse Order</field>
        <field name="res_model">import.warehouse.order</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_import_warehouse_order_wizard"/>
        <field name="context">{}</field>
        <field name="target">new</field>
    </record>


            <!--    Export warehouse order    -->
