        <field name="model">import.warehouse.order</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group string="Files for warehouse order ">

                        <div>
                        <p>import_warehouse_order_file(name).csv <a href="import_bridge_axis/static/description/warehouse Order/import_warehouse_order_file(name).csv" ><i class="fa fa-download"></i></a></p>
                        <p>import_warehouse_order_file(name).xls <a href="import_bridge_axis/static/description/warehouse Order/import_warehouse_order_file(name).xls" ><i class="fa fa-download"></i></a></p>

<!--                        </div>-->
<!--                        <div>-->
                        <p>import_warehouse_order_file(barcode).csv <a href="import_bridge_axis/static/description/warehouse Order/import_warehouse_order_file(barcode).csv" ><i class="fa fa-download"></i></a></p>
                        <p>import_warehouse_order_barcode.xls <a href="import_bridge_axis/static/description/warehouse Order/import_warehouse_order_barcode.xls" ><i class="fa fa-download"></i></a></p>
