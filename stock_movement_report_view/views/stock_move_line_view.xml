<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_stock_movement_tree" model="ir.ui.view">
        <field name="name">stock movement tree view</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_move_line_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="replace">
                <tree decoration-muted="state == 'cancel'" decoration-danger="(state not in ('cancel','done'))"
                      string="Move Lines" default_order="x_trans_date desc">
                    <!--                    <field name="date"/>-->
                    <!--                    <field name="reference" string="Reference"-->
                    <!--                           invisible="context.get('no_reference', False)"/>-->
                    <!--                    <field name="product_id"/>-->
                    <!--                    <field name="lot_id" groups="stock.group_production_lot"/>-->
                    <!--                    <field name="location_id"/>-->
                    <!--                    <field name="location_dest_id"/>-->
                    <!--                    <field name="qty_done" string="Quantity Done"/>-->
                    <!--                    <field name="product_uom_id" options="{'no_open': True, 'no_create': True}" string="Unit of Measure"-->
                    <!--                           groups="uom.group_uom"/>-->
                    <field name="state" invisible="1"/>
                    <field name="company_id" groups="base.group_multi_company" force_save="1" invisible="1"/>
                    <field name="x_war"/>
                    <field name="x_ccode"/>
                    <field name="owner_id"/>
                    <field name="product_id"/>
                    <field name="x_name"/>
                    <field name="barcode"/>
                    <field name="x_hs"/>
                    <field name="x_cntr_code"/>
                    <field name="x_tno"/>
                    <field name="container_no"/>
                    <field name="x_trans_type"/>
                    <field name="origin"/>
                    <field name="x_trans_date"/>
                    <field name="x_trans_no"/>
<!--                    <field name="loc"/>-->
                    <field name="loc_code"/>
                    <field name="x_uom"/>
                    <field name="x_qty"/>
                    <field name="x_vol"/>
                    <field name="total_gw" digits="[5,3]"/>
                    <field name="total_nw" digits="[5,3]"/>
                    <field name="x_namee"/>
                    <field name="pallet"/>
                    <field name="x_ref_date"/>
                    <field name="x_ref_no"/>
                    <field name="x_bill"/>
                    <field name="item_boe"/>
                    <field name="batchno"/>
                    <field name="lot_id"/>
                    <field name="production_date"/>
                    <field name="expiry_date"/>
                    <field name="x_coo"/>

                </tree>
            </xpath>
        </field>

    </record>
</odoo>