<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>

        <record id="cargo_status_report_view" model="ir.ui.view">
        <field name="name">cargo.status.wizard.form</field>
        <field name="model">cargo.status.wizard</field>
        <field name="arch" type="xml">
            <form>

                <group>
                    <field name="start_date"/>
                    <field name="end_date"/>
                    <field name="partner_id" widget="many2many_tags" invisible="1"/>


                </group>
                <footer>
                    <button string="Excel" type="object" name="print_excel" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
    </data>
</odoo>