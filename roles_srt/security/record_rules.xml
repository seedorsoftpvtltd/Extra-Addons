<record id="csf_job_restrict" model="ir.rule">
    <field name="name">Job Booking:CFS</field>
    <field name="model_id" ref="scs_freight.model_freight_operation"/>
    <field name="groups" eval="[(4, ref('roles_srt.group_csf_job_restrict'))]"/>
    <field name="domain_force">[('x_job_type', 'in', ('CFS'))]</field>
</record>
     <record id="freight_job_restrict" model="ir.rule">
    <field name="name">Job Booking:Freight</field>
    <field name="model_id" ref="scs_freight.model_freight_operation"/>
    <field name="groups" eval="[(4, ref('roles_srt.group_freight_job_restrict'))]"/>
    <field name="domain_force">[('x_job_type', 'in', ('Freigth'))]</field>

</record>
     <record id="relocation_job_restrict" model="ir.rule">
