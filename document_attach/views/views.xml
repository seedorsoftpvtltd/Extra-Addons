<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <record id="document_attach_form" model="ir.ui.view">
            <field name="name">document.attach.form</field>
            <field name="model">document.attach</field>
            <field eval="7" name="priority" />
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                       <group string="Main Import Sheet">
                             <field name="text4"/>
                            <field name="attachment_field"/>
                       </group>
                        <group string="Import Guidance">
                            <field name="text"/>
                       </group>
                        </group>
                    <group>
                        <group string="Dependency Import Sheet">
                            <field name="text1"/>
                            <field name="attachment1_field"/>
                            <field name="text2"/>
                            <field name="attachment2_field"/>
                            <field name="text3"/>
                            <field name="attachment3_field"/>
                       </group>
                    </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="document_attach_tree" model="ir.ui.view">
            <field name="name">document.attach.tree</field>
            <field name="model">document.attach</field>
            <field eval="7" name="priority" />
            <field name="arch" type="xml">
                <tree>
                            <field name="text4"/>

                </tree>
            </field>
        </record>
        <record id="document_attach_kanban" model="ir.ui.view">
            <field name="name">document.attach.kanban</field>
            <field name="model">document.attach</field>
            <field name="arch" type="xml">
                <kanban class="o_kanban">

                       <field name="text4"/>
                       <templates>
                                  <t t-name="kanban-box">
                                     <div class="oe_kanban_global_click">
                                         <div class="kanban_overtime">
                                           <strong class="o_kanban_record_title oe_partner_heading">
                                               <field name="text4"/>
                                           </strong>
                                         </div>


                                    </div>

                                  </t>
                       </templates>
                </kanban>

            </field>
        </record>

     <record id="action_document_attach" model="ir.actions.act_window">
        <field name="name">Import</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">document.attach</field>
        <field name="view_mode">tree,form,kanban</field>
    </record>

    <menuitem id="document_atatch_id"
              name="Import"
              action="action_document_attach"/>

</odoo>