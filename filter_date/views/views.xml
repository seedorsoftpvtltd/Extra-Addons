<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
        <record id="view_partner_inherit_followup_form12" model="ir.ui.view">
			<field name="name">res.partner</field>
			<field name="model">res.partner</field>
			<field name="inherit_id" ref="base.view_partner_form"/>
			<field name="arch" type="xml">
				<xpath expr="//page/field[@name='balance_invoice_over_ids']" position="before">
					<group>
						<field name="filter_start_date"/>
						<field name="filter_end_date"/>
					    <field name="currency"/>
<!--						<button name="apply_filter" type="object" string="Apply"/>-->
						<!--button name="print_report" type="object"
								string="Print Filterd Report"
								help="Print Filterd Report"
								 /-->

					</group>
				</xpath>
				<xpath expr="//page/field[@name='balance_invoice_ids']" position="before">
					<group>
						<field name="filter_start_date_customer"/>
						<field name="filter_end_date_customer"/>
						<field name="currency_customer"/>

						<!--button name="print_report_customer" type="object"
								string="Print Filterd Report"
								help="Print Filterd Report"
								 /-->

					</group>
				</xpath>
				<xpath expr="//page/field[@name='supplier_invoice_ids']" position="before">
					<group>
						<field name="filter_start_date_vendor"/>
						<field name="filter_end_date_vendor"/>
						<field name="currency_vendor"/>

						<!--button name="print_report_vendor" type="object"
								string="Print Filterd Report"
								help="Print Filterd Report"
								 /-->

					</group>
				</xpath>
			</field>
		</record>
	</data>
</odoo>
