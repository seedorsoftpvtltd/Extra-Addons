<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_kanban_with_onboarding" model="ir.ui.view">
        <field name="name">goods.issue.order.kanban.onboard</field>
        <field name="model">goods.issue.order</field>
        <field name="inherit_id" ref="gio.view_goods_order_kanban"/>
        <field name="arch" type="xml">
            <xpath expr="//kanban" position="attributes">
                <attribute name="banner_route">/gio/onboarding_panel</attribute>
            </xpath>
        </field>
    </record>
</odoo>