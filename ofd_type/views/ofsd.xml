<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="ofd_packages_form" model="ir.ui.view">
        <field name="name">ofd.packages.form</field>
        <field name="model">ofd.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>

                </sheet>
            </form>
        </field>
    </record>


    <record id="ofd_packages_tree" model="ir.ui.view">
        <field name="name">ofd.packages.tree</field>
        <field name="model">ofd.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>


    <record id="ofd_packages_action" model="ir.actions.act_window">
        <field name="name">Type of ofd</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">ofd.type</field>
        <field name="view_mode">tree,form</field>
    </record>


</odoo>