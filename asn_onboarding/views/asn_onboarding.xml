<odoo>
    <!-- ONBOARDING STEPS -->

    <template id="asn_step1">
        <t t-call="asn_onboarding.onboarding_default_step">
            <t t-set="title">Customers</t>
            <t t-set="description">
                Create / Upload the Customer Data

            </t>
            <t t-set="sl">1</t>
                       <t t-set="btn_text1">Add</t> <t t-set="btn_text3">Import</t>
                        <t t-set="method1" t-value="'onboarding_action_asn_create_approve'" />
                        <t t-set="method2" t-value="'onboarding_action_asn_import_approve'" />
                        <t t-set="model" t-value="'res.company'" />
        </t>
    </template>

    <template id="asn_step2">
