<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="s_centric_subscriber" name="Subscriber">
		<section class="centric_template">
			<section class="service-banner s_cover parallax s_parallax_is_fixed pt96 s_parallax_no_overflow_hidden bg-black-75 pb120" data-scroll-background-ratio="1" data-name="Cover" style="background-image: none;">
		       <span class="s_parallax_bg oe_img_bg oe_custom_bg" style="background-image: url('/theme_centric/static/src/img/2862664.png'); background-position: 46.59% 50.53%;"/>
		       <div class="container">
					<div class="subs-content text-center">
						<span class="discount">Get Started Instantly!</span>
						<h2>Get the only new update from this newsletter</h2>
						
					</div>
					<div class="input-group js_subscribe" data-list-id="1">
				        <input type="email" name="email" class="js_subscribe_email form-control" placeholder="your email..."/>
				        <span class="input-group-append">
				            <a role="button" href="#" class="btn btn-primary js_subscribe_btn">Subscribe</a>
				            <a role="button" href="#" class="btn btn-success js_subscribed_btn d-none" disabled="disabled">Thanks</a>
				        </span>
				    </div>
				</div>
		     </section>
		 </section>
	</template>
</odoo>


