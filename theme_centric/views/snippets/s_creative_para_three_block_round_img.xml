									</section>
								</div>
								<div class="col-md-4">
									<section class="card card-shadow">
										<div class="card-body text-center text-uppercase">
											<span
												class="fa fa-pie-chart display-5 text-alpha"></span>
											<h6 class="m-t-10 font-medium">survive</h6>
										</div>
									</section>
								</div>
							</div>
							<div class="s_btn" data-name="Button">
