												data-subscribe="off">
												<div class="m-b-20">
													<input type="email" name="email"
														class="js_subscribe_email form-control"
														placeholder="enter email" />

												</div>
												<a role="button" href="#"
													class="btn  btn-feature rounded-circle js_subscribe_btn o_default_snippet_text">JOIN US</a>
												<a role="button" href="#"
													class="btn btn-success js_subscribed_btn d-none o_default_snippet_text"
