<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="website_multi_footer_six"
		inherit_id="portal.frontend_layout" name="Footer Six" active="False">
		<xpath expr="//footer" position="replace">
			<footer id="footer_six" class="footer_six o_footer">
				<div class="footer3 font-14">
					<div class="f3-topbar container">
						<div class="d-flex">
							<div class="d-flex no-block align-items-center">
								<span>
									Lorem ipsum dolor sit amet,
									<span class="text-white">consectetur adipiscing elit</span>
									, sed do eiusmod
									<br />
									tempor incididunt ut labore et dolore magna ad minim.
								</span>
							</div>
							<div class="ml-auto no-shrink align-self-center">
								<form class="news_letter_form">
									<div class="input-group js_subscribe" data-list-id="1"
										data-subscribe="off">
										<input type="email" name="email"
											class="js_subscribe_email form-control form-control-dark form-control-lg"
											placeholder="Sign up for updates" />

										<span class="input-group-append input-group-btn">
											<a role="button" href="#"
												class="btn btn-feature btn-md js_subscribe_btn ">Go!</a>
											<a role="button" href="#"
												class="btn btn-success js_subscribed_btn d-none o_default_snippet_text"
												disabled="disabled">Thanks</a>
										</span>
										<div
											class="alert alert-success d-none o_default_snippet_text"
											role="status">Thanks for your subscription!</div>
									</div>
								</form>
							</div>
						</div>
					</div>
					<div class="f3-middle container">
						<div class="row">
							<div class="col-lg-3 col-md-6 m-b-30">
								<h6 class="font-medium text-white">SERVICES</h6>
								<ul class="general-listing">
									<li>
										<a href="#">
											<i class="fa fa-1x fa-fw fa-long-arrow-right ml-1"></i>
											Website Design
										</a>
									</li>
									<li>
										<a href="#">
											<i class="fa fa-1x fa-fw fa-long-arrow-right ml-1"></i>
											Mobile App Development
										</a>
									</li>
									<li>
										<a href="#">
											<i class="fa fa-1x fa-fw fa-long-arrow-right ml-1"></i>
											Search Engine Optimization
										</a>
									</li>
									<li>
										<a href="#">
											<i class="fa fa-1x fa-fw fa-long-arrow-right ml-1"></i>
											Website Development
										</a>
									</li>
								</ul>
							</div>
							<div class="col-lg-3 col-md-6 m-b-30">
								<h6 class="font-medium text-white">PROJECTS</h6>
								<div class="d-flex no-block align-items-center m-t-20">
									<div class="thumb m-r-20">
										<img src="/theme_centric/static/src/img/footer1.jpg"
											alt="" />
									</div>
									<div class="btext">
										<a href="#" class="link">Hotel nira inn got bigger heding
											you ever
											seen.
										</a>
									</div>
								</div>
								<div class="d-flex no-block align-items-center m-t-20">
									<div class="thumb m-r-20">
										<img src="/theme_centric/static/src/img/footer2.jpg"
											alt="" />
									</div>
									<div class="btext">
										<a href="#" class="link">Hotel nira inn got bigger heding
											you ever
											seen.
										</a>
									</div>
								</div>
							</div>
							<div class="col-lg-3 col-md-6 m-b-30">
								<h6 class="font-medium text-white">BLOG</h6>
								<div class="d-flex no-block align-items-center m-t-20">
									<div class="thumb m-r-20">
										<img src="/theme_centric/static/src/img/footer3.jpg"
											alt="" />
									</div>
									<div class="btext">
										<a href="#" class="link">Hotel nira inn got bigger heding
											you ever
											seen.
										</a>
									</div>
								</div>
								<div class="d-flex no-block align-items-center m-t-20">
									<div class="thumb m-r-20">
										<img src="/theme_centric/static/src/img/footer4.jpg"
											alt="" />
									</div>
									<div class="btext">
										<a href="#" class="link">Hotel nira inn got bigger heding
											you ever
											seen.
										</a>
									</div>
								</div>
							</div>
							<div class="col-lg-3 col-md-6 m-b-30">
								<h6 class="font-medium text-white">CONTACT</h6>
								<div class="d-flex no-block m-b-10 m-t-20">
									<div class="display-7 m-r-20 align-self-top">
										<i class="fa fa-map-marker"></i>
									</div>
									<div class="info">
										<p>
											<span t-field="res_company.street"></span>
											,
											<br />
											<span t-field="res_company.city"></span>
											,
											<span t-field="res_company.zip"></span>
											,
											<br />
											<span t-field="res_company.country_id.name"></span>

										</p>
									</div>
								</div>
								<div class="d-flex no-block m-b-10">
									<div class="display-7 m-r-20 align-self-top">
										<i class="fa fa-phone"></i>
									</div>
									<div class="info">
										<span class=" db  m-t-5" t-if="res_company.phone"
											t-field="res_company.phone"></span>
									</div>
								</div>
								<div class="d-flex no-block m-b-30">
									<div class="display-7 m-r-20 align-self-top">
										<i class="fa fa-envelope"></i>
									</div>
									<div class="info">
										<a href="#" class="link db  m-t-5" t-if="res_company.email"
											t-field="res_company.email"></a>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="f3-bottom-bar">
						<div class="container">
							<div class="d-flex">
								<span class="m-t-10 m-b-10">
									Copyright 2017. All Rights Reserved by
									<span t-field="res_company.name" />
									.
								</span>
								<div class="ml-auto m-t-10 m-b-10">
									<a class="link" t-att-href="website.social_facebook"
										t-if="website.social_facebook" target="_blank">
										<i class="fa fa-facebook" />
									</a>
									<a class="link" t-att-href="website.social_twitter"
										t-if="website.social_twitter" target="_blank">
										<i class="fa fa-twitter" />
									</a>
									<a class="link" t-att-href="website.social_linkedin"
										t-if="website.social_linkedin" target="_blank">
										<i class="fa fa-linkedin" />
									</a>
									<a class="link" t-att-href="website.social_youtube"
										t-if="website.social_youtube" target="_blank">
										<i class="fa fa-youtube-play" />
									</a>
									<a class="link" t-att-href="website.social_instagram"
										t-if="website.social_instagram" target="_blank">
										<i class="fa fa-instagram"></i>
									</a>
									<a class="link" t-att-href="website.social_github"
										t-if="website.social_github" target="_blank">
										<i class="fa fa-github"></i>
									</a>
								</div>
							</div>
						</div>
					</div>
					<div id="to-top">
						<a id="to-top bg-alpha">
							<span class="fa fa-arrow-up"></span>
						</a>
					</div>
				</div>
			</footer>
		</xpath>
	</template>
</odoo>