<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_warehouse_tag_inherit_form" model="ir.ui.view">
        <field name="name">warehouse.order.form</field>
       	<field name="model">warehouse.order</field>
      	<field name="inherit_id" ref="warehouse.warehouse_order_form"/> 
	<field name="arch" type="xml">
	    <xpath expr="//field[@name='partner_ref']" position="after">
	        <field name="tag_ids" widget="many2many_tags"/>
	    </xpath>				
 	</field>		
    </record>
    <record id="view_warehouse_tags_tree" model="ir.ui.view">
        <field name="name">warehouse.tag.tree</field>
	<field name="model">warehouse.tag</field>
	<field name="arch" type="xml">
	    <tree string="Tags">
		 <field name="name"/>		
	    </tree>
 	</field>		
     </record>

    <record id="view_warehouse_tags_form" model="ir.ui.view">
        <field name="name">warehouse.tag.form</field>
	<field name="model">warehouse.tag</field>
	<field name="arch" type="xml">
	    <form string="Tags">
	        <group>
		    <field name="name"/>
	       	</group>
	    </form>
 	</field>		
    </record>
	
    <record id="open_view_warehouse_tags" model="ir.actions.act_window">
        <field name="name">Tags</field>
	<field name="res_model">warehouse.tag</field>
	<field name="view_mode">tree,form</field>
	<field name="view_id" ref="view_warehouse_tags_tree" />
    </record>
    <menuitem id="menu_warehouse_tags" name="warehouse Booking Tag " parent="warehouse.menu_warehouse_config" action="open_view_warehouse_tags" sequence="21"/>
</odoo>




