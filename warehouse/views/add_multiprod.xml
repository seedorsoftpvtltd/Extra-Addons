<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_order_form_warehouse_multi_ept" model="ir.ui.view">
        <field name="name">warehouse.order.form.view</field>
        <field name="model">warehouse.order</field>
        <field name="inherit_id" ref="warehouse.warehouse_order_form" />
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button name="open_multi_product_selection_wizard" type="object"
                        attrs="{'invisible':[('state', '!=', 'draft')]}"
                        string="Add Multi Goods"/>
            </xpath>
        </field>
    </record>
    <record id="view_warehouse_multi_products_wizard" model="ir.ui.view">
            <field name="name">Select Multiple Goods Wizard</field>
            <field name="model">warehouse.multi.products</field>
            <field name="arch" type="xml">
                <form string="Multi Products Selection" version="7">
                    <group>
                        <field name="do_replace"/>
                    </group>
                    <field name="product_ids"/>
                    <footer>
                        <button name="warehouse_multi_products" string="Apply" type="object"
                                class="oe_highlight"/>
                        <button string="_Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>
</odoo>