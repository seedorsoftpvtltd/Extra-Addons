<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="sh_customer_rfq_portal_assets_frontend_pack" inherit_id="portal.assets_frontend">
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/sh_customer_rfq_portal_pack/static/src/js/portal.js" />
        </xpath>
    </template>
    <template id="sh_portal_request_customer_rfq_product_pack" name="Customer RFQ" inherit_id="sh_customer_rfq_portal.sh_portal_request_customer_rfq_product">
        <xpath expr="//table[@id='rfq_table']//thead//tr//th[2]" position="after">
            <th class="text-center">Pack QTY</th>
        </xpath>
    </template>
</odoo>
