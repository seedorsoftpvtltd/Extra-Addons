    <template id="successful_feedback" name="Success Page">
        <t t-call="website.layout">
            <div id="wrap">
                <br/><br/>
                <div class="oe_structure"/>
                <div class="container">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="alert alert-info">
                                <strong> Your Feedabck have been sent successfully.<br/>
                                    Thank you,<br/>
                                    Adminstrator
                                </strong>
                            </div>
                            <a class="btn btn-primary pull-left mb32" 
                            onclick="history.go(-1);" >
                                <span class="hidden-xs">Go Back</span>
                                <span class="fa fa-long-arrow-left"/>
                            </a>
                        </div>
                        <div class="col-md-6">
                            <img width="200" height="200" 
                            src="/project_request_for_information/static/description/success.png" 
                            class="img-square" />
                        </div>
                    </div>
                </div>
            </div>
        </t>
