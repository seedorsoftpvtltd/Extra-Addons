<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <report 
            id="action_report_goodsorder"
            string="Goods Order"
            model="goods.issue.order"
            report_type="qweb-pdf"
            file="gio.report_goodsorder"
            name="gio.report_goodsorder"
            print_report_name="(object.state in ('draft', 'sent') and 'Quotation - %s' % (object.name)) or 'Order - %s' % (object.name)"
        />

        <report
            id="action_goods_report_pro_forma_invoice"
            string="PRO-FORMA Invoice"
            model="gio.order"
            report_type="qweb-pdf"
            groups="gio.group_proforma_goods"
            file="gio.report_goodsorder_pro_forma"
            name="gio.report_goodsorder_pro_forma"
            print_report_name="'PRO-FORMA - %s' % (object.name)"
        />
    </data>
</odoo>
