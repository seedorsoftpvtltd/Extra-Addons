<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="held_products.act_window__held_products" model="ir.actions.act_window" context="{'studio': True}">
    <field name="binding_model_id" eval="False"/>
    <field name="binding_type">action</field>
    <field name="binding_view_types">list,form</field>
    <field name="context">{'search_default_x_partner_id': active_id,'default_x_partner_id': active_id}</field>
    <field name="domain">[('x_partner_id', '=', active_id)]</field>
    <field name="filter" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="help"><![CDATA[<p><br></p>]]></field>
    <field name="limit">80</field>
    <field name="name">Held Products</field>
    <field name="res_model">stock.quant</field>
    <field name="search_view_id" eval="False"/>
    <field name="target">current</field>
    <field name="type">ir.actions.act_window</field>
    <field name="usage" eval="False"/>
    <field name="view_id" eval="False"/>
    <field name="view_mode">tree,form</field>
  </record>
  <record id="held_products.wa__held_products" model="ir.actions.act_window" context="{'studio': True}">
