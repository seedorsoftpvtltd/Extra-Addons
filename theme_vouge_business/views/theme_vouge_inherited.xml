	<template id="button_style_options" inherit_id="website.snippet_options">
		<xpath expr="//div[@data-js='colorpicker'][1]" position="after">
        <div data-selector=".btn"
            data-exclude=".btn-link">
            <we-collapse-area>
                <we-toggler><i class="fa fa-magic fa-font"/>Button Style Option</we-toggler>
                <we-collapse>
                    <we-button data-select-class="default_style">Default Style</we-button>
                      <we-button data-select-class="bizople_button_1">Bizople Button 1</we-button>
                      <we-button data-select-class="bizople_button_2">Bizople Button 2</we-button>
                      <we-button data-select-class="bizople_button_3">Bizople Button 3</we-button>
                      <we-button data-select-class="bizople_button_4">Bizople Button 4</we-button>
                      <we-button data-select-class="bizople_button_5">Bizople Button 5</we-button>
                      <we-button data-select-class="bizople_button_6">Bizople Button 6</we-button>
                      <we-button data-select-class="bizople_button_7">Bizople Button 7</we-button>
                      <we-button data-select-class="bizople_button_8">Bizople Button 8</we-button>
                      <we-button data-select-class="bizople_button_9">Bizople Button 9</we-button>
                      <we-button data-select-class="bizople_button_10">Bizople Button 10</we-button>
                      <we-button data-select-class="bizople_button_11">Bizople Button 11</we-button>
                      <we-button data-select-class="bizople_button_12">Bizople Button 12</we-button>
                </we-collapse>
            </we-collapse-area>
        </div>
    </xpath>

		<xpath expr="//div[@data-js='colorpicker'][1]" position="after">
      <div data-selector=".biz_dynamic_product_slider, .oe_multi_category_slider, .bizcommon_category_slider_3, .blog_slider_owl">
            <we-collapse-area>
                <we-toggler><i class="fa fa-magic fa-font"/> Slider Width</we-toggler>
                <we-collapse>
                    <we-button data-select-class="default_style">Default Size</we-button>
                      <we-button data-select-class="extended-container">Extended size</we-button>
                </we-collapse>
            </we-collapse-area>
        </div>
    </xpath>
	</template>

  <template id="cookie_options" inherit_id="website.snippet_options" name="Cookie Snippets Option Inherit">
    <xpath expr="//div[@id='so_main_colorpicker']" position="before">
      <div data-selector="#website_cookies_bar" data-js="CookiesBar" data-target=".modal">
        <we-customizeblock-option>
          <we-collapse-area>
                <we-toggler string="Layout" class="snippet-option-layout_column">Layout</we-toggler>
                <we-collapse data-no-preview="true">
                    <we-button data-select-class="o_cookies_discrete" data-mode="discrete" class="o_cookies_discrete">Discrete</we-button>
                    <we-button data-select-class="o_cookies_classic" data-mode="classic" class="o_cookies_classic">Classic</we-button>
                    <we-button data-name="layout_popup_opt" data-select-class="o_cookies_popup" data-mode="popup" data-trigger="position_middle,s_popup_size_md" class="o_cookies_popup">Popup</we-button>
                  </we-collapse>
                </we-collapse-area>
            </we-customizeblock-option>

            <we-customizeblock-option>
              <we-collapse-area>
                <we-toggler string="position" class="snippet-option-layout_column">Position</we-toggler>
                <we-collapse data-no-preview="true">
                  <we-select string="Position" data-dependencies="layout_popup_opt"> <!-- When cookie configuration only display this for popup mode -->
                    <we-button data-select-class="s_popup_top" data-name="position_top">Top</we-button>
                        <we-button data-select-class="s_popup_middle" data-name="position_middle">Middle</we-button>
                        <we-button data-select-class="s_popup_bottom" data-name="position_bottom">Bottom</we-button>
                    </we-select>
                  </we-collapse>
                </we-collapse-area>
              </we-customizeblock-option>
              
              <we-customizeblock-option>
                <we-collapse-area>
                  <we-toggler string="Backdrop">Backdrop</we-toggler>
                  <we-collapse data-no-preview="true">
                    <we-button  data-select-class="visible">Visible</we-button>
                    <we-button  data-select-class="notvisible">Not Visible</we-button>
                    </we-collapse>
                  </we-collapse-area>
                </we-customizeblock-option>
            </div>

            <div data-selector="#website_cookies_bar .modal-content">
              <we-customizeblock-option>
              <we-collapse-area>
                <we-toggler string="size" class="snippet-option-layout_column">Size</we-toggler>
                <we-collapse data-no-preview="true">
                  <we-select string="Size" data-apply-to=".modal-dialog" data-name="s_popup_size_opt">
                      <we-button data-select-class="modal-sm">Small</we-button>
                      <we-button data-select-class="" data-name="s_popup_size_md">Medium</we-button>
                      <we-button data-select-class="modal-lg">Large</we-button>
                      <we-button data-select-class="modal-xl">Extra Large</we-button>
                      <we-button data-select-class="s_popup_size_full" data-name="s_popup_size_full">Full</we-button>
                  </we-select>
                </we-collapse>
              </we-collapse-area>
            </we-customizeblock-option>
          </div>
      </xpath>
  </template>
</odoo>
