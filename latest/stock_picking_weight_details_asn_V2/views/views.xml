<?xml version="1.0" encoding="utf-8" ?>

<odoo>

    <record id="warehouse_packages_form_inherited_extend" model="ir.ui.view">
        <field name="name">warehouse.order.inherited</field>
        <field name="model">warehouse.order</field>
        <field name="inherit_id" ref="asn_views.view_asn_views_extend_model"/>
        <field name="arch" type="xml">
            <xpath expr="//page[1]//field[@name='price_unit']" position="after">
                        <field name="net_weight" invisible="1"/>
                <field name="gross_weight" invisible="1"/>
                <field name="volume" invisible="1"/>
            </xpath>
            <xpath expr="//page[1]//field[@name='price_unit']" position="before">

                        <field name="value_goods" invisible="1"/>
                <field name="currency_id" invisible="1" string=" "/>
            </xpath>
        </field>
    </record>
</odoo>