<?xml version="1.0" encoding="utf-8" ?>

<odoo>

    <record id="warehouse_packages_form_inherited_extend" model="ir.ui.view">
        <field name="name">warehouse.order.inherited</field>
        <field name="model">warehouse.order</field>
        <field name="inherit_id" ref="asn_views.view_asn_views_extend_model"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='currency_id']" position="after">

                <field name="type_of_package" domain="['|', ('company_id', '=', False), ('company_id', '=',company_id)]" widget="many2many_tags"  attrs="{'invisible': [('x_transport', 'not in', ('air',))]}"/>


            </xpath>

        </field>
    </record>
</odoo>
