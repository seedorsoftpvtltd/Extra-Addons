<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

<record id="view_asn_views_extend_model" model="ir.ui.view">
			<field name="name">warehouse.order</field>
			<field name="model">warehouse.order</field>
			<field name="inherit_id" ref="warehouse.warehouse_order_form" />
			<field name="arch" type="xml">
				<xpath expr="//form" position="replace">
                    <form string="Advance Shipping Note" class="o_warehouse_order">
                        <header>
                    <button name="action_Booking_send" states="draft" string="Send by Email" type="object" context="{'send_Booking':True}" class="oe_highlight"/>
                    <button name="print_quotation" string="Print Booking" type="object" states="draft" class="oe_highlight" groups="base.group_user"/>
                    <button name="button_approve" type="object" states="to approve" string="Approve Order" class="oe_highlight" groups="warehouse.group_warehouse_manager"/>
                    <button name="action_Booking_send" states="sent" string="Re-Send by Email" type="object" context="{'send_Booking':True}"/>
                    <button name="button_confirm" type="object" states="sent" string="Confirm Order" class="oe_highlight" id="bid_confirm"/>
                    <button name="print_quotation" string="Print Booking" type="object" states="sent" groups="base.group_user"/>
                    <button name="action_Booking_send" states="warehouse" string="Send Booking by Email" type="object" context="{'send_Booking':False}"/>
                    <button name="button_confirm" type="object" states="draft" string="Confirm Order" id="draft_confirm"/>
                    <button name="button_cancel" states="draft,to approve,sent,warehouse" string="Cancel" type="object"/>
                    <button name="button_done" type="object" string="Lock" states="warehouse"/>
                    <button name="button_unlock" type="object" string="Unlock" states="done" groups="warehouse.group_warehouse_manager"/>
                    <field name="state" widget="statusbar" statusbar_visible="draft,sent,warehouse" readonly="1" invisible="1"/>
                </header>
                <sheet>
                    <div class="oe_button_box" name="button_box">
                    </div>
                    <div class="oe_title">
                        <span class="o_form_label" attrs="{'invisible': [('state','not in',('draft','sent'))]}">Advance Shipping Note</span>
                        <span class="o_form_label" attrs="{'invisible': [('state','in',('draft','sent'))]}">Advance Shipping Note</span>
                        <h1>
                            <field name="name" readonly="1"/>
                        </h1>
                    </div>
                    <group>
                        <group>
                          <field name="x_medium" readonly="0" string="ASN Type" options="{'no_create': True, 'no_create_edit':True}"/>
                            <field name="partner_id" widget="res_partner_many2one" context="{'res_partner_search_mode': 'supplier', 'show_vat': True}" placeholder="Name, TIN, Email, or Reference"/>
                            <field name="x_clientno" readonly="0" attrs="{'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                            <field name="partner_ref" string="Client Ref / LPO No" attrs="{'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                            <field name="x_consignee" widget="res_partner_many2one" readonly="0" attrs="{'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                            <field name ="x_shipper" readonly="0" attrs="{'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                            <field name="currency_id" groups="base.group_multi_currency" force_save="1"/>

                                <field name="x_bill" readonly="0" attrs="{'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>

                                    <field name="x_qty" string="Received Qty" readonly="0" attrs="{'invisible':[('x_medium','in',(12,13,14))], 'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                                     <field name="x_manifest_qty" readonly="0" attrs="{'invisible':[('x_medium','in',(12,13,14))], 'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                                  <field name="x_wt" readonly="0" attrs="{'invisible':[('x_medium','in',(12,13,14))],'required': [('x_medium', '=',16)], 'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])] }"/>
                                    <field name="x_vol" digits="[5,3]" readonly="0" attrs="{'invisible':[('x_medium','in',(12,13,14))], 'required': [('x_medium', '=',16)], 'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}" />
                                    <field name="x_coo_port" readonly="0" string="Port of Origin"  attrs="{'invisible':[('x_medium','in',(12,13,14))], 'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                                  <field name="x_cof_dest_port" readonly="0" string="Port of Final Dtn"  attrs="{'invisible':[('x_medium','in',(12,13,14))], 'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                                  <field name="x_remark" readonly="0" attrs="{'invisible':[('x_medium','in',(12,13,14))], 'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>


                        </group>
                        <group>
                           <field name="date_order" string="ETA" invisible="1" readonly="0"/>
                            <field name="date_order" attrs="{'invisible': [('state','=','warehouse')]}" string="Order Date"/>
                            <field name="date_approve" attrs="{'invisible': [('state','!=','warehouse')]}"/>
                            <field name="origin" attrs="{'invisible': [('origin','=',False)]}"/>

                            <field name="origin" readonly="0" invisible="1" attrs="{'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                            <field name="x_flight" readonly="0" string="Vessel / Flight" attrs="{'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                            <field name="x_hbl" readonly="0" string="BL / AWB" attrs="{'invisible':[('x_medium','not in',(12,13,14))], 'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                            <field name='x_master_bl_asn' string="MBL No" readonly='False' attrs="{'invisible':[('x_medium','in',(12,13,14))], 'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                            <field name='x_house_bl_asn' string="HBL No" readonly='False' attrs="{'invisible':[('x_medium','in',(12,13,14))],  'required': [('x_medium', '=',16)], 'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                          <field name="x_suppliername" string="Supplier Name" attrs="{'invisible':[('x_medium','in',(12,13,14))], 'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>

                            <field name="x_ware" readonly="0" options="{'no_create': True, 'no_create_edit':True}" attrs="{'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                             <field name="company_id" groups="base.group_multi_company" options="{'no_create': True}" invisible="1" attrs="{'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>

                              <field name="x_marksnos" string="Marks &amp; Nos" attrs="{'invisible':[('x_medium','in',(12,13,14))]}"/>
                                <field name="container_qty" invisible="0" attrs="{'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                               <field name="container" string="Container Type" invisible="1" attrs="{'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                               <field name="container_no" invisible="0" attrs="{'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>
                                   <field name="x_placeof_delivery" string="Place Of Delivery" readonly="0" attrs="{'invisible':[('x_medium','in',(12,13,14))], 'readonly': [('state', 'in', ['warehouse', 'done', 'cancel'])]}"/>

                        </group>

                    </group>
                    <notebook>
                        <page string="Goods">
                            <field name="order_line" widget="section_and_note_one2many" mode="tree,kanban" context="{'default_state': 'draft'}" attrs="{'readonly': [('state', 'in', ('done', 'cancel'))]}">
                              <button name="open_multi_product_selection_wizard" type="object" string="Add Multi Goods"/>
                                <tree string="Advance Shipping Note Lines" limit="50" editable="bottom">
                                    <control>
                                        <create name="add_product_control" string="Add a Good"/>
                                        <create name="add_section_control" string="Add a section" context="{'default_display_type': 'line_section'}"/>
                                        <create name="add_note_control" string="Add a note" context="{'default_display_type': 'line_note'}"/>
                                    </control>
                                    <field name="display_type" invisible="1"/>
                                    <field name="currency_id" invisible="1"/>
                                    <field name="state" invisible="1" readonly="1"/>
                                    <field name="product_type" invisible="1"/>
                                    <field name="product_uom_category_id" invisible="1"/>
                                    <field name="sequence" widget="handle"/>
                                    <field name="x_test" invisible="1"/>
                                    <field name="product_id" string="Product Name" domain="[('customer_id','=',parent.partner_id)]"/>
                                    <field name="name" string="Description" readonly="0" invisible="1"/>
                                    <field name='x_volume1' digits="[5,3]"/>
                                    <field name="x_desc"  string="Product Description" />
                                    <field name="product_qty" readonly="0" />

                                    <field name="date_planned" width="400" invisible="1" readonly='0'/>
                                    <field name="account_analytic_id" optional="hide" context="{'default_partner_id':parent.partner_id}" groups="analytic.group_analytic_accounting" domain="['|', ('company_id', '=', False), ('company_id', '=', parent.company_id)]"/>
                                    <field name="analytic_tag_ids" optional="hide" groups="analytic.group_analytic_tags" widget="many2many_tags" options="{'color_field': 'color'}" domain="['|', ('company_id', '=', False), ('company_id', '=', parent.company_id)]"/>
                                    <field name="qty_received_manual" invisible="1"/>
                                    <field name="qty_received_method" invisible="1"/>
                                    <field name="qty_received" string="Received" invisible="1" attrs="{'column_invisible': [('parent.state', 'not in', ('warehouse', 'done'))], 'readonly': [('qty_received_method', '!=', 'manual')]}" optional="show"/>
                                    <field name="product_uom" string="Pack Code" groups="uom.group_uom" readonly='0' optional="show"/>
                                    <field name="price_unit" invisible="1"/>
                                    <field name="production_date" invisible="1" string="Production Date" readonly="0"/>
                                    <field name="expiry_date"  invisible="1" readonly="0"/>
                                    <field name="taxes_id"  invisible="1" widget="many2many_tags" domain="[('type_tax_use','=','none'), ('company_id', '=', parent.company_id)]"  options="{'no_create': True}" optional="show"/>
                                    <field name="price_subtotal" width="400" widget="monetary" invisible="1"/>

                                </tree>
                                <form string="Advance Shipping Note Line">
                                        <field name="state" invisible="1"/>
                                                                        <field name="x_h" string="H.S CODE" readonly="1"/>
                                        <field name="display_type" invisible="1"/>
                                        <group attrs="{'invisible': [('display_type', '!=', False)]}">
                                            <group>
                                                <field name="product_uom_category_id" invisible="1"/>
                                                <field name="product_id"  readonly="1" context="{'partner_id': parent.partner_id}" widget="many2one_barcode" domain="[('purchase_ok', '=', True), '|', ('company_id', '=', False), ('company_id', '=', parent.company_id)]"/>
                                                <label for="product_qty"/>
                                                <div class="o_row">
                                                    <field name="product_qty"/>
                                                   <field name="product_uom" groups="uom.group_uom" attrs="{'required': [('display_type', '=', False)]}"/>
                                                </div>
                                                <field name="qty_received_method" invisible="1"/>
                                                <field name="qty_received" string="Received Quantity" attrs="{'invisible': [('parent.state', 'not in', ('warehouse', 'done'))], 'readonly': [('qty_received_method', '!=', 'manual')]}"/>
                                                <field name="price_unit" invisible="1"/>
                                    <field name="taxes_id" widget="many2many_tags" domain="[('type_tax_use','=','none'), ('company_id', '=', parent.company_id)]"  options="{'no_create': True}" optional="show"/>
                                            </group>
                                            <group>
                                                <field name="date_planned" widget="date" attrs="{'required': [('display_type', '=', False)]}"/>
                                                <field name="account_analytic_id" colspan="2" domain="['|', ('company_id', '=', False), ('company_id', '=', parent.company_id)]" groups="analytic.group_analytic_accounting"/>
                                                <field name="analytic_tag_ids" groups="analytic.group_analytic_tags" domain="['|', ('company_id', '=', False), ('company_id', '=', parent.company_id)]" widget="many2many_tags" options="{'color_field': 'color'}"/>
                                                <field name="company_id" groups="base.group_multi_company" options="{'no_create': True}"/>
                                            </group>
                                            <group colspan="12">
                                            <notebook>
                                                <page string="Notes">
                                                    <field name="name"/>
                                                </page>
                                            </notebook>
                                            </group>
                                        </group>
                                        <label for="name" string="Section Name (eg. Goods, Services)" attrs="{'invisible': [('display_type', '!=', 'line_section')]}"/>
                                        <label for="name" string="Note" attrs="{'invisible': [('display_type', '!=', 'line_note')]}"/>
                                        <field name="name" nolabel="1" attrs="{'invisible': [('display_type', '=', False)]}"/>
                                 </form>
                                 <kanban class="o_kanban_mobile">
                                     <field name="name"/>
                                     <field name="product_id"/>
                                     <field name="product_qty"/>
                                     <field name="product_uom" groups="uom.group_uom"/>
                                     <field name="price_subtotal" invisible="1"/>
                                     <field name="price_tax" invisible="1"/>
                                     <field name="price_total" invisible="1"/>
                                     <field name="price_unit" invisible="1"/>
                                     <field name="display_type"/>
                                     <field name="taxes_id" invisible="1"/>
                                     <templates>
                                         <t t-name="kanban-box">
                                             <div t-attf-class="oe_kanban_card oe_kanban_global_click {{ record.display_type.raw_value ? 'o_is_' + record.display_type.raw_value : '' }}">
                                                 <t t-if="!record.display_type.raw_value">
                                                     <div class="row">
                                                         <div class="col-8">
                                                             <strong>
                                                                 <span t-esc="record.product_id.value"/>
                                                             </strong>
                                                         </div>
                                                         <div class="col-4">
                                                             <strong>
                                                                 <span t-esc="record.price_subtotal.value" class="float-right text-right"/>
                                                             </strong>
                                                         </div>
                                                     </div>
                                                     <div class="row">
                                                         <div class="col-12 text-muted">
                                                             <span>
                                                                 Quantity:
                                                                 <t t-esc="record.product_qty.value"/>
                                                                 <t t-esc="record.product_uom.value"/>
                                                             </span>
                                                         </div>
                                                     </div>
                                                     <div class="row">
                                                         <div class="col-12 text-muted">
                                                             <span>
                                                                 Unit Price:
                                                                 <t t-esc="record.price_unit.value"/>
                                                             </span>
                                                         </div>
                                                     </div>
                                                 </t>
                                                 <div t-elif="record.display_type.raw_value === 'line_section' || record.display_type.raw_value === 'line_note'" class="row">
                                                     <div class="col-12">
                                                         <span t-esc="record.name.value"/>
                                                     </div>
                                                 </div>
                                             </div>
                                         </t>
                                     </templates>
                                 </kanban>
                            </field>
                            <group class="oe_subtotal_footer oe_right" invisible="1">
                                <field name="amount_untaxed" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                                <field name="amount_tax" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                                <div class="oe_subtotal_footer_separator oe_inline">
                                    <label for="amount_total"/>
                                </div>
                                <field name="amount_total" invisible="1" nolabel="1" class="oe_subtotal_footer_separator" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                            </group>
                            <field name="notes" class="oe_inline" placeholder="Define your terms and conditions ..."/>
                            <div class="oe_clear"/>
                        </page>
                        <page string="Transport Details" invisible="1">
                          <group>
                                                        <field name="x_mode"/>
                            <field name="transporter_id"/>
                                               <field name="shipper_name" pattern="[a-zA-Z]"/>

                          </group>


                        </page>
                        <page string="Other Information" name="warehouse_delivery_invoice">
                            <group>
                                <group name="planning">
                                    <label for="date_planned"/>
                                    <div>
                                        <field name="date_planned" attrs="{'readonly': [('state', 'not in', ('draft', 'sent'))]}"/>
                                    </div>
                                </group>
                            </group>
                        </page>
                    </notebook>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="activity_ids" widget="mail_activity"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
                    </form>
                </xpath>
            </field>
</record>
    </data>
</odoo>