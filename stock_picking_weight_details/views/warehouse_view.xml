<?xml version="1.0" encoding="utf-8" ?>

<odoo>

    <record id="warehouse_packages_form_inherited" model="ir.ui.view">
        <field name="name">warehouse.order.inherited</field>
        <field name="model">warehouse.order</field>
        <field name="inherit_id" ref="warehouse.warehouse_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[1]//field[@name='price_unit']" position="after">
                <field name="net_weight"/>
                <field name="gross_weight"/>
                <field name="volume"/>
            </xpath>
            <xpath expr="//page[1]//field[@name='price_unit']" position="before">

                <field name="value_goods"/>
                <field name="currency_id" invisible="0" string=" "/>
            </xpath>
        </field>
    </record>
</odoo>