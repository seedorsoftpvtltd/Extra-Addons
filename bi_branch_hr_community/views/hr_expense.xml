<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    
        <record id="bi_branch_hr_community_inherit_hr_expense_view_form" model="ir.ui.view">
            <field name="name">bi.branch.hr.community.inherit.hr.expense.view.form</field>
            <field name="model">hr.expense</field>
            <field name="inherit_id" ref="hr_expense.hr_expense_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='company_id']" position="after">
                    <field name="branch_id"/>
                </xpath>
            </field>
        </record>
        
        
        <record id="bi_branch_hr_community_inherit_view_expenses_tree" model="ir.ui.view">
            <field name="name">bi.branch.hr.community.inherit.view.expenses.tree</field>
            <field name="model">hr.expense</field>
            <field name="inherit_id" ref="hr_expense.view_expenses_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='employee_id']" position="after">
                    <field name="branch_id"/>
                </xpath>
            </field>
        </record>
        
    </data>
</odoo>