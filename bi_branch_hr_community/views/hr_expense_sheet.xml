<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    
        <record id="bi_branch_hr_community_inherit_view_hr_expense_sheet_form" model="ir.ui.view">
            <field name="name">bi.branch.hr.community.inherit.view.hr.expense.sheet.form</field>
            <field name="model">hr.expense.sheet</field>
            <field name="inherit_id" ref="hr_expense.view_hr_expense_sheet_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='company_id']" position="after">
                    <field name="branch_id"/>
                </xpath>
            </field>
        </record>
        
        
        <record id="bi_branch_hr_community_inherit_view_hr_expense_sheet_tree" model="ir.ui.view">
            <field name="name">bi.branch.hr.community.inherit.view.hr.expense.sheet.tree</field>
            <field name="model">hr.expense.sheet</field>
            <field name="inherit_id" ref="hr_expense.view_hr_expense_sheet_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='employee_id']" position="after">
                    <field name="branch_id"/>
                </xpath>
            </field>
        </record>
        
    </data>
</odoo>