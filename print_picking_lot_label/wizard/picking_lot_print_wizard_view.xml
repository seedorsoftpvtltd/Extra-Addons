<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
        <record model="ir.ui.view" id="view_picking_lot_print_wizard_form">
           <field name="name">picking.lot.print.wizard.form</field>
           <field name="model">picking.lot.print.wizard</field>
           <field name="arch" type="xml">
               <form string="Print Lot Label">
                    <p>This wizard will print number of lot labels as per defined quantity.</p>
                    <group>
                        <field name="quantity"/>
                    </group>
                    <footer>
                        <button string="Print" name="action_print_lot_label" type="object" default_focus="1" class="btn-primary"/> or 
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
           </field>
       </record>
       
