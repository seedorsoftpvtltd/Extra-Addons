<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="0">
		<record id="ir_cron_auto_generate_invoice" model="ir.cron">
	        <field name="name">Contract Invoice Generation</field>
	        <field name="interval_number">1</field>
	        <field name="interval_type">days</field>
	        <field name="numbercall">-1</field>
	        <field name="nextcall" eval="(DateTime.now().replace(hour=5, minute=0) + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"  />
	        <field name="doall" eval="False"/>
	        <field name="model_id" ref="model_account_analytic_account"/>
	        <field name="code">model._cron_generate_invoice()</field>
	        <field name="state">code</field>
    	</record>

    	 <record id="default_template" model="ir.config_parameter">
            <field name="key">sales_contract_seperate_module.default_template</field>
            <field name="value" ref="sales_contract_seperate_module.mail_template_contract"/>
        </record>
		
	</data>
</odoo>