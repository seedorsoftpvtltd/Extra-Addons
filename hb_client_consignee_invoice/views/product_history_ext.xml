<odoo>

      <record id="view_prod_history_inh_form_ccinv" model="ir.ui.view">
            <field name="name">product.history.inherit</field>
            <field name="model">product.history</field>
            <field name="inherit_id" ref="hb_wms_invoice_v1.view_prod_history_inh_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='charge_unit_type']" position="before">

						<field name="gen_seperate_inv" />
