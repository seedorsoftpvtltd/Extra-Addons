<odoo>
    <data>
    <record id="generate_invoice" model="ir.ui.view" >
    <field name="name">Generate Invoice</field>
    <field name="model">generate.invoice</field>
    <field name="type">form</field>
    <field name="arch" type="xml">
        <form string="Generate Invoice">
          <group>
                 <field name="partner_id" domain="[('company_id','=', company_id)]"/>
                <field name="company_id" invisible="1" options="{'no_create': True, 'no_open': True}"/>
            </group>
            <group>
                <field name="start_date" />
                <field name="end_date" />
            </group>

            <footer>
                <button name="generate_invoice" string="Generate Invoice" type="object" class="oe_highlight" />

                <button string="Cancel" special="cancel"/>
            </footer>
        </form>
    </field>
</record>

<act_window id="action_generate_invoice"
    name="Generate Invoice"
    res_model="generate.invoice"
    view_mode="form"
    target="new"/>



<menuitem action="action_generate_invoice"
    id="generate_invoicee"
    parent="account.menu_finance_receivables" />

    </data>

</odoo>
